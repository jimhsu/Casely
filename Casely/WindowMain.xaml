<Window x:Class="Casely.WindowMain"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Casely"
        mc:Ignorable="d"
        Title="Casely" Height="450" Width="800" Loaded="Window_Loaded">
    <Grid>
        <Button x:Name="btnGrossing" Content="Grossing" Margin="10,65.724,0,0" Height="58.527" VerticalAlignment="Top" Click="btnGrossing_Click" HorizontalAlignment="Left" Width="283.835"/>
        <Button x:Name="btnSignout" Content="Signout" Margin="10,129.251,0,0" Click="btnSignout_Click" Height="59.165" VerticalAlignment="Top" HorizontalAlignment="Left" Width="283.835"/>
        <TextBlock HorizontalAlignment="Left" Margin="33.293,33,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="275.804" Text="What would you like to track today?" FontSize="14" d:LayoutOverrides="HorizontalAlignment"/>
        <Button x:Name="btnCompare" Content="Check Reports" Margin="330.262,65.086,229.903,0" Click="btnCompare_Click" Height="59.165" VerticalAlignment="Top"/>
        <TextBlock HorizontalAlignment="Left" Margin="338.293,33,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="275.804" Text="What analysis would you like to do?" FontSize="14"/>
        <Button x:Name="btnSearch" Content="Search Reports" Margin="330.262,129.251,229.903,0" Click="btnCompare_Click" Height="59.165" VerticalAlignment="Top"/>
        <Button x:Name="btnDiagnosis" Content="Diagnosis" Margin="10,193.416,0,166.419" Click="btnDiagnosis_Click" HorizontalAlignment="Left" Width="283.835"/>
        <Menu Height="19" Margin="0,0,10,0" VerticalAlignment="Top">
            <MenuItem Header="File">
                <MenuItem Header="Import Soft Path Tab Separated Data" Click="MenuItem_Click_1"></MenuItem>
            </MenuItem>
        </Menu>
        <ListBox x:Name="lbRecentlyAdded" Height="144.419" Margin="10,0" VerticalAlignment="Bottom"/>
        <TextBlock HorizontalAlignment="Left" Margin="10,0,0,147.799" TextWrapping="Wrap" VerticalAlignment="Bottom" Width="275.804" Text="Recently added cases" FontSize="14" d:LayoutOverrides="HorizontalAlignment"/>

    </Grid>
</Window>
